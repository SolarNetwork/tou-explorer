{
	"name": "tou-explorer",
	"private": true,
	"version": "0.0.0",
	"author": {
		"name": "Matt Magoffin",
		"email": "matt@solarnetwork.net"
	},
	"license": "Apache-2.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview"
	},
	"ava": {
		"files": [
			"src/test/ts/**/*"
		],
		"typescript": {
			"rewritePaths": {
				"src/": "lib/"
			},
			"compile": "tsc"
		}
	},
	"c8": {
		"exclude": [
			"src/test/ts/**"
		],
		"exclude-after-remap": true,
		"reporter": [
			"text",
			"html"
		]
	},
	"dependencies": {
		"@popperjs/core": "^2.11.8",
		"bootstrap": "^5.3.2",
		"nifty-tou": "github:SolarNetwork/nifty-tou#main",
		"papaparse": "^5.4.1",
		"solarnetwork-api-core": "^1.0.0"
	},
	"devDependencies": {
		"@ava/typescript": "^4.1.0",
		"@types/papaparse": "^5.3.8",
		"@typescript-eslint/eslint-plugin": "^6.7.2",
		"@typescript-eslint/parser": "^6.7.2",
		"ava": "^5.3.1",
		"c8": "^8.0.1",
		"eslint": "^8.49.0",
		"sass": "^1.67.0",
		"typedoc": "^0.25.1",
		"typescript": "^5.2.2",
		"vite": "^4.4.9"
	}
}
